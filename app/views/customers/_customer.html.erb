<div id="<%= dom_id customer %>">
  <p>
    <strong><i class="fa-solid fa-user"></i> </strong>
    <%= customer.full_name %>
  </p>

  <p>
    <strong><i class="fa-solid fa-phone"></i> </strong>
    <%= customer.phone %>
  </p>

  <p>
    <strong><i class="fa-solid fa-at"></i> </strong>
    <%= customer.email %>
  </p>

</div>
